@{
    ViewData["Title"] = "Home Page";
}

<h2>@ViewBag.Message</h2>
<h3>
    @ViewData["message"]
</h3>
<p>
    @ViewData["Experience"]

</p>

<small>@ViewData["year"] </small>
<ol>
    @foreach (string color in (IEnumerable<string>)ViewData["list"])
    {
        <li>
            @color
        </li>

    }
</ol>

<ul>
    @foreach (var item in ViewBag.ColorList)
    {
        <li>
            @item
        </li>
    }
</ul>
